  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && pnpm run build-assets && pnpm run build-server",
    "start": "node devServer.js",
    "start-remote": "cross-env ANIMETA_CONFIG_PATH=./config.remote.json npm run start",
    "start-prod": "npm run build-assets && npm run build-server && cp assets.json ../frontend-server/ && cd ../frontend-server && ts-node --project tsconfig.json frontendServer.js",
    "build-assets": "webpack --env.prod --progress",
    "build-assets:analyze": "webpack --env.prod --progress --stats --json > stats.json && webpack-bundle-analyzer stats.json ../static/build",
    "build-server": "webpack --env.server --env.prod",
    "graphql-codegen": "graphql-codegen --watch",
    "test": "cd frontend; mocha --require ts-node/register test"
  },
  "msw": {
    "workerDirectory": "static"
  }
